generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}
model Place {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  category    String
  longitude   Float
  latitude    Float
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
